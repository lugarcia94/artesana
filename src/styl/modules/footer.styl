footer
    padding 0 !important
    .bg-grey
        padding 40px 0
        background-color: #f7f7f7
    .footer
        &__wrapper
            display: flex !important
            justify-content: space-between
            max-width: 1245px !important
            .fbits-footer-blocos-vertical
                flex: 0 1 25%
                ul.list-social
                    display: flex 
                    align-items: center
                    margin-top 20px
                    li
                        .face-icon,
                        .blog-icon,
                        .twit-icon,
                        .you-icon
                            display: block
                            width: 20px
                            height: 20px
                            background-repeat: no-repeat
                            background-position: center
                            background-size 20px
                            margin-right 15px
                        .face-icon
                            background-image: url('data:image/svg+xml;base64,')
                        .blog-icon
                            background-image: url('data:image/svg+xml;base64,')
                        .twit-icon
                            background-image: url('data:image/svg+xml;base64,')
                        .you-icon
                            background-image: url('data:image/svg+xml;base64,')
                    
            .titlefooter
                color: $cor01
                font-weight: 600
            .linksfooter
                float: none
                img 
                    margin-bottom: 15px
                a
                    font-size: 13px
                    font-weight 500
                    text-transform uppercase
                    color: $cor01
                span 
                    display: block
                    line-height 30px
                    color: $cor01
                    &.tel-icon
                    &.whats-icon
                        font-size: 18px
                        font-weight 800
                        display flex
                        align-items: center
                        margin: 10px 0
                    &.tel-icon
                        &:before
                            content: ''
                            display: block
                            width: 15px
                            height: 20px
                            background-image: url('data:image/svg+xml;base64,')
                            background-repeat: no-repeat
                            background-position: center
                            background-size 15px 20px
                            margin-right 7px
                    &.whats-icon
                        color: #45b547
                        &:before
                            content: ''
                            display: block
                            width: 18px
                            height: 20px
                            background-image: url('data:image/svg+xml;base64,')
                            background-repeat: no-repeat
                            background-position: center
                            background-size 18px 
                            margin-right 5px
    .fbits-footer-blocos-horizontal
        max-width: 1245px !important
        margin 0 auto !important
        float: none !important
        .dadosloja
            max-width 780px
            p   
                margin-bottom 5px
                font-size: 11px
                &.copy
                    margin-top: 20px
        .credits 
            display: flex
            justify-content: flex-end
        