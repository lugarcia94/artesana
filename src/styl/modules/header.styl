.header
    position: fixed
    top: 0
    left: 0
    width: 100%
    z-index: 99
    background-color: #fff
    height: initial
    .topBar
        background-color: #ffffff
        border: 1px solid #cecece
        height: 30px
        > div
            display initial
            padding: initial
        &__container
            display: flex !important
            justify-content: space-between
            max-width: 1245px
            margin: 0 auto
            height: 30px
        &__left
            display flex
            align-items: center
            justify-content: space-between
            flex: 0 1 390px
            span,
            a
                font-size: 11px
                color: #818181
            strong  
                color: #818181
        &__right
            display flex
            justify-content: space-between
            align-items: center
            flex: 0 1 450px
            a
                font-size: 11px
                color: #818181
        &__info
            display: flex 
            justify-content: space-between
            align-items: center
            flex: 0 1 360px
            span 
                color: #ffffff
                font-size: 13px
                display flex
                align-items: center
        &__whatsapp
            display: flex
            align-items: center
            color: #57ca6f !important
            &:before
                content: ''
                display: block;
                margin-right 5px
                min-width: 13px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: 13px;
                background-position center
            &:after
                content: '|'
                display: inline-block
                color: #ffffff
                padding-left 21px
        &__email
            &:before
                content: ''
                margin-right 5px
                width: 15px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: auto 
                background-position center
    &__main
        background-color: #fff
        padding 15px 0
        .align-items
            width: 100%
            padding 0
            display flex
            justify-content: space-between
            align-items: center
    &__container
        display: flex !important
        justify-content: space-between
        max-width: 1245px
        margin: 0 auto
    &__nav
        background-color: #f5f5f5
    .headerLogo
        margin-left 30px
        img     
            max-width: 133px
    .div-busca
        flex 0 1 608px
        .headerSearch 
            width: 100%
            .inputSearch[type="text"]
                width: 100%
                height 46px
                border: 1px solid #0080c7 !important
                border-radius: 5px
        button#btnBuscaMobile
            font-size: 12px
            font-weight bold
            color: #fff
            text-transform uppercase
            background-color #0080c7
            background-image: none
            width: 90px
            height: 46px
            margin: 0
            border-radius: 0 5px 5px 0
    .top-icons
        .user-login
            span    
                font-size: 12px