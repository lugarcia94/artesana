.header
    position: fixed
    top: 0
    left: 0
    width: 100%
    z-index: 99
    background-color: #fff
    height: initial
    .topBar
        background-color: #ffffff
        border: 1px solid #cecece
        height: 30px
        transition: .3s
        @media(max-width: 991px)
            display: none
        > div
            display initial
            padding: initial
        &__container
            display: flex !important
            justify-content: space-between
            max-width: 1245px
            margin: 0 auto
            height: 30px
            @media(max-width: 1299px)
                max-width: 991px
        &__left
            display flex
            align-items: center
            justify-content: space-between
            flex: 0 1 390px
            span,
            a
                font-size: 11px
                color: #818181
            strong  
                color: #818181
        &__right
            display flex
            justify-content: space-between
            align-items: center
            flex: 0 1 450px
            a
                font-size: 11px
                color: #818181
        &__info
            display: flex 
            justify-content: space-between
            align-items: center
            flex: 0 1 360px
            span 
                color: #ffffff
                font-size: 13px
                display flex
                align-items: center
        &__whatsapp
            display: flex
            align-items: center
            color: #57ca6f !important
            &:before
                content: ''
                display: block;
                margin-right 5px
                min-width: 13px
                min-height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: 13px;
                background-position center
            &:after
                content: '|'
                display: inline-block
                color: #ffffff
                padding-left 21px
        &__email
            &:before
                content: ''
                margin-right 5px
                width: 15px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: auto 
                background-position center
    &__main
        background-color: #fff
        padding 10px 0
        z-index: 1;
        position: relative;
        .align-items
            width: 100%
            padding 0
            display flex
            justify-content: space-between
            align-items: center
            @media(max-width: 991px)
                flex-wrap: wrap
                height: initial !important
                .div-busca
                    width: 100%
                    max-width: initial 
                    flex: initial
                    order: 5
    &__container
        display: flex !important
        justify-content: space-between
        max-width: 1245px
        margin: 0 auto
        @media(max-width: 1299px)
            max-width: 991px
            padding 0 15px
    &__wrapper
        display: flex 
        justify-content: space-between
        align-items: center
        width 100%
        height 40px

    &__nav
        background-color: #f5f5f5
        height: 40px
        transition: .3s
        @media(max-width: 991px)
            .menu-nav
                flex: 0 1 250px !important
                ul.menu
                    > li.item.todas-categorias
                        display: none !important
            &-right 
                width: 50px !important
                a
                    &.ofertas-btn
                        font-size: 12px !important
                .calc-btn
                    display: none
                .ofertas-btn
                    color: #e58822 !important
                    background-color: transparent !important
        .menu-nav
            flex: 0 1 770px
            @media(max-width: 1299px)
                flex: 0 1 640px
        &-right
            display: flex
            align-items: center 
            justify-content: flex-end
            width: 375px
            height: 40px
            a
                display: flex
                align-items: center
                height 100%
                font-size: 15px
                font-weight: 600
                &.ofertas-btn
                    justify-content: center
                    width: 105px
                    background-color: $cor02
                    color: #fff
                &.calc-btn
                    color: $cor02
                    opacity 0
                    &:before
                        content: ''
                        margin-right 5px
                        width: 20px
                        height: 20px
                        background-image: url('data:image/svg+xml;base64,')
                        background-repeat: no-repeat
                        background-size: auto 
                        background-position center
                    
    
    .headerLogo
        margin-left 30px
        width: 133px
        max-width: 133px
        img     
            max-width: 133px
            width: 100%;
        @media(max-width: 1299px)
            margin-left: 0
        @media(max-width: 768px)
            width: 100px
    .div-busca
        flex 0 1 608px
        @media(max-width: 1299px)
            flex 0 1 480px
        .headerSearch 
            width: 100%
            @media (max-width: 990px)
                margin: 0 !important
            .inputSearch[type="text"]
                width: 100%
                height 46px
                border: 1px solid #0080c7 !important
                border-radius: 5px
                padding-left 40px
                font-size: 14px
                background-image: url('data:image/svg+xml;base64,')
                background-size: 16px
                background-repeat no-repeat
                background-position-x: 15px
                background-position-y center
                @media(max-width: 991px)
                    font-size 12px !important
                    &::placeholderr
                        font-size 12px !important
        button#btnBuscaMobile
            font-size: 12px
            font-weight bold
            color: #fff
            text-transform uppercase
            background-color #0080c7
            background-image: none
            width: 90px
            height: 46px
            margin: 0
            border-radius: 0 5px 5px 0
            @media(max-width: 991px)
                width: 85px
    .top-icons
        width: 375px
        @media(max-width: 991px)
            width: 150px
            &.icons-right
                display: flex !important
                justify-content: flex-end !important
                top: initial !important
            .ico-carrinho
                right: initial !important
        @media(max-width: 768px)
            width: 80px !important
            &.icons-right
                display: flex !important
                float: none !important 
                right: initial !important
            > div
                width: 40px !important
        &.icons-right
            justify-content: space-between
            
        .user-login
            span    
                font-size: 12px
                color:#7f7f7f
        > div
            width: 115px
            height: initial;
            margin-left: 0
            font-family: 'Exo', sans-serif
            float: none
            .headerLogin 
                display: flex !important
                justify-content: center 
                align-items: center 
                width: 100% !important
                background-image: initial
                padding 0
                &:before
                    content: ''
                    display block
                    min-width: 28px
                    margin-right: 5px
                    height 28px
                    background-image: url('data:image/svg+xml;base64,')
                    background-repeat: no-repeat
                    background-position: center 
                    background-size: 28px
                    margin-right 15px
                    @media(max-width: 1299px)
                        height 32px
                    @media(max-width: 991px)
                        margin-right: 0
                .loginHead
                    display block
                    height: 41px
                    @media(max-width: 991px)
                        display none
                    .textloginp.usuario_nao_identificado
                        display flex
                        flex-wrap: wrap
                        font-size: 0
                        span 
                            display: block
                            text-transform: initial
                            &:before    
                                content: 'OlÃ¡,'
                                margin-right 5px
                            &:after    
                                content: 'Entrar'
                                display block
                                font-size: 16px;
                                font-weight: 800;
                                line-height: 8px;
                                letter-spacing: 1px
        .ico-carrinho,
        .carrinhoHead
            width: 190px !important
            float: none
            *
                float: none
                font-family: 'Exo', sans-serif !important
            span.minicart-txt-itens
                display block
                font-size: 13px
                font-weight bold
                color: #7f7f7f
            @media(max-width: 991px)
                width 50px !important
                height: 30px
                background-image: none !important
                float: none !important
                img 
                    position relative !important
                    top: initial !important
                    left: initial !important
                #fbits-resumo-carrinho
                    display: none
        .ico-carrinho
            img
                width 30px
                height 30px
                position absolute
                top: 8px;
                left: 10px;
                z-index: 1;
            &:before
                display none
        .carrinhoHead
            position relative
            a
                height: 46px
                background-color #fff
                box-shadow: 0px 0px 4px #a7a7a7;
                border-radius: 5px;
                display flex
                flex-direction: column
                justify-content: center
                background-image: url('data:image/svg+xml;base64,')
                background-size: 12px
                background-repeat no-repeat
                background-position-x: 93%
                background-position-y center
                span 
                    position relative !important
                    right: initial !important
                    bottom: initial !important
                    top: initial !important
                    text-align: left 
                    padding-left 50px
                    height: 15px;
                    margin-top -5px
                    &.minicart-qtde-itens
                        display flex
                        justify-content: flex-start
                        align-items: center
                        text-transform initial !important
                        color: #7f7f7f !important
                        @media(max-width: 1299px)
                            font-size: 12px;
                        &:after 
                            display block !important
                            content: 'item(s)'
                            display: inline-block
                            margin-left: 5px

.icomobile-hamburguer
    display none !important
    position: relative !important
    top: initial !important
    left: initial !important
    background-image: url('data:image/svg+xml;base64,') !important
    background-repeat: no-repeat
    background-position: center
    width: 35px !important
    height: 35px !important
    @media(max-width: 991px)
        display: block !important
        width: 25px !important
        height: 25px !important

.menu-mobile
    position fixed 
    top: 0
    left: -350px
    z-index 99
    background-color: #f5f5f5
    width: 300px
    height 100vh
    transition: .3s
    .menu-mobile__helpers
        display flex
    .close-menu
        width: 30px
        height: 30px
        background-image: url('data:image/svg+xml;base64,')
        background-repeat: no-repeat
        background-size: 18px
        background-position center
    &__header
        display flex
        justify-content: space-between
        align-items: center
        padding 15px
        a,
        span 
            font-size: 12px
            &:after
                display: none !important
        .tel-icopn
            display flex
            align-items: center
            color: #828282 !important
            &:before
                content: ''
                width: 10px
                height: 10px
                display block
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: 10px
                margin-right: 5px
                margin-left 10px

    &__nav
        ul.menu
            flex-wrap: wrap
            padding: 0 15px
            height initial
            li.item.raiz.todas-categorias
                display none !important
            > li.item.raiz.item0
                width: 100% !important
                border-top: 1px solid #cecece
                height 40px !important
                a
                    font-size: 16px !important
                &:nth-child(2)
                    order: initial !important
                    max-width: initial !important
                    a
                        font-size: 16px !important
                &:nth-child(3)
                    order: initial !important
                    max-width: initial !important
                    a
                        font-size: 16px !important
                &:nth-child(4)
                    order: initial !important
                    max-width: initial !important
                    a
                        font-size: 16px !important
                &:nth-child(5)
                    display block !important
                    a
                        font-size: 16px !important
                &:nth-child(6)
                    display block !important
                    a
                        font-size: 16px !important
    .header__nav-right
        padding: 10px 15px !important
        flex-wrap: wrap
        max-width: 100% !important
        max-height:initial !important
        width: 100% !important
        height:initial !important
        a
            font-size: 16px !important
            font-weight: 500 !important
            height 40px !important
            display flex !important
            align-items: center
            border-top: 1px solid #cecece
        a.calc-btn
            max-width: 100% !important
            width: 100% !important
            flex-direction: row-reverse
            justify-content: space-between !important
        a.ofertas-btn
            max-width: 100% !important
            width: 100% !important
            text-align: left !important
            justify-content: flex-start
            border-bottom: 1px solid #cecece

.mask-mobile
    position: absolute 
    display: none
    top: 0
    left: 0
    width: 100%
    height: 100vh
    background-color: rgba(0,0,0,.5)
    z-index 1