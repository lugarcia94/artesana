.header
    position: fixed
    top: 0
    left: 0
    width: 100%
    z-index: 99
    background-color: #fff
    height: initial
    .topBar
        background-color: #ffffff
        border: 1px solid #cecece
        height: 30px
        > div
            display initial
            padding: initial
        &__container
            display: flex !important
            justify-content: space-between
            max-width: 1245px
            margin: 0 auto
            height: 30px
        &__left
            display flex
            align-items: center
            justify-content: space-between
            flex: 0 1 390px
            span,
            a
                font-size: 11px
                color: #818181
            strong  
                color: #818181
        &__right
            display flex
            justify-content: space-between
            align-items: center
            flex: 0 1 450px
            a
                font-size: 11px
                color: #818181
        &__info
            display: flex 
            justify-content: space-between
            align-items: center
            flex: 0 1 360px
            span 
                color: #ffffff
                font-size: 13px
                display flex
                align-items: center
        &__whatsapp
            display: flex
            align-items: center
            color: #57ca6f !important
            &:before
                content: ''
                display: block;
                margin-right 5px
                min-width: 13px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: 13px;
                background-position center
            &:after
                content: '|'
                display: inline-block
                color: #ffffff
                padding-left 21px
        &__email
            &:before
                content: ''
                margin-right 5px
                width: 15px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: auto 
                background-position center
    &__main
        background-color: #fff
        padding 10px 0
        .align-items
            width: 100%
            padding 0
            display flex
            justify-content: space-between
            align-items: center
    &__container
        display: flex !important
        justify-content: space-between
        max-width: 1245px
        margin: 0 auto
    &__wrapper
        display: flex 
        justify-content: space-between
        align-items: center
        width 100%
        height 40px
    &__nav
        background-color: #f5f5f5
        height: 40px
        .menu-nav
            flex: 0 1 770px
    
    .headerLogo
        margin-left 30px
        width: 133px
        max-width: 133px
        img     
            max-width: 133px
            width: 100%;
    .div-busca
        flex 0 1 608px
        .headerSearch 
            width: 100%
            .inputSearch[type="text"]
                width: 100%
                height 46px
                border: 1px solid #0080c7 !important
                border-radius: 5px
                padding-left 40px
                font-size: 14px
                background-image: url('data:image/svg+xml;base64,')
                background-size: 16px
                background-repeat no-repeat
                background-position-x: 15px
                background-position-y center
        button#btnBuscaMobile
            font-size: 12px
            font-weight bold
            color: #fff
            text-transform uppercase
            background-color #0080c7
            background-image: none
            width: 90px
            height: 46px
            margin: 0
            border-radius: 0 5px 5px 0
    .top-icons
        width: 375px
        &.icons-right
            justify-content: space-between
        .user-login
            span    
                font-size: 12px
                color:#7f7f7f
        > div
            width: 115px
            height: initial;
            margin-left: 0
            font-family: 'Exo', sans-serif
            float: none
            .headerLogin 
                display: flex !important
                justify-content: center 
                align-items: center 
                width: 100% !important
                background-image: initial
                padding 0
                &:before
                    content: ''
                    display block
                    min-width: 28px
                    margin-right: 5px
                    height 28px
                    background-image: url('data:image/svg+xml;base64,')
                    background-repeat: no-repeat
                    background-position: center 
                    background-size: 28px
                    margin-right 15px
                .loginHead
                    display block
                    height: 41px
                    .textloginp.usuario_nao_identificado
                        display flex
                        flex-wrap: wrap
                        font-size: 0
                        span 
                            display: block
                            text-transform: initial
                            &:before    
                                content: 'Ol√°,'
                                margin-right 5px
                            &:after    
                                content: 'Entrar'
                                display block
                                font-size: 16px;
                                font-weight: 800;
                                line-height: 8px;
                                letter-spacing: 1px
        .ico-carrinho,
        .carrinhoHead
            width: 190px !important
            float: none
            *
                float: none
                font-family: 'Exo', sans-serif !important
            span.minicart-txt-itens
                display block
                font-size: 13px
                font-weight bold
                color: #7f7f7f
        .ico-carrinho
            img
                width 30px
                height 30px
                position absolute
                top: 8px;
                left: 10px;
                z-index: 1;
        .carrinhoHead
            position relative
            a
                height: 46px
                background-color #fff
                box-shadow: 0px 0px 4px #a7a7a7;
                border-radius: 5px;
                display flex
                flex-direction: column
                justify-content: center
                background-image: url('data:image/svg+xml;base64,')
                background-size: 12px
                background-repeat no-repeat
                background-position-x: 93%
                background-position-y center
                span 
                    position relative !important
                    right: initial !important
                    bottom: initial !important
                    top: initial !important
                    text-align: left 
                    padding-left 50px
                    height: 15px;
                    margin-top -5px
                    &.minicart-qtde-itens
                        display flex
                        justify-content: flex-start
                        align-items: center
                        text-transform initial !important
                        color: #7f7f7f !important
                        &:after 
                            display block !important
                            content: 'item(s)'
                            display: inline-block
                            margin-left: 5px