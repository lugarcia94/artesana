.category
    &__header
        &-container
            max-width 1200px
            width: 100%
            margin: 0 auto
            
        &-wrapper
            display: flex
            align-items: center
            justify-content: space-between
            flex-wrap: wrap
            padding: 0
    @media(max-width 1290px)
        max-width: 991px
        margin 0 auto
    &__breadcrumb
        >ol
            display: inline-block
            >li
                display: inline
                &:first-child
                    span[itemprop]
                        font-size: 0px
                        text-transform uppercase
                        color: #989898
                        font-weight 400
                        &:before
                            content: 'Home'
                            font-size: 10px
                span[itemprop]
                    font-size: 10px
                    color: #979797
                    font-weight: 800
                    line-height: 16px
                    text-transform uppercase

                .separador
                    &:before
                        content: ''
                        display inline-block
                        color: #979797
                        width: 10px
                        height 10px
                        background-image: url('data:image/svg+xml;base64,')
                        background-repeat no-repeat
                        background-position: center 
                        background-size: 8px

                &:last-child
                    span[itemprop]
                        font-weight: bold
                        color: $cor02
    &__inforesult
        font-size: 13px
        color: #333333
        font-weight: 500
        line-height: 16px
        

    &__ordenation
        display flex
        justify-content: flex-end;

        .fbits-ordenar
            display: flex
            align-items: center
        .fbits-ordenar-por
            font-size: 14px;
            line-height: 30px;
            color: #333333;
            font-weight: 700;
            text-align: right;
            white-space: nowrap

            @media(max-width: 767px)
                display none
        select 
            height 30px
            background-color: #ffffff
            border: 1px solid #F0F0F0
            border-radius: 0
            color: #333333
            font-size: 13px
            appearance none
            padding-left: 9px
            background-image url('data:image/svg+xml;base64,') !important
            background-size 10px
            width 250px

            @media(max-width: 767px)
                width auto
    &__pagination
        .fbits-paginacao > ul
            a
                display: inline-block
                width: 35px
                height 35px
                line-height: 35px
                text-align: center
                font-size: 16px
                color: #373535
                font-weight: 400
                background: white
                margin: 0
                border: 1px solid #d9d9d9;
            li
                display: inline
                &.sel a
                    color: #ffffff
                    background: #111111

    &__list
        &-container
            max-width 1200px
            width: 100%
            margin: 0 auto
        &-wrapper
            display: flex
            align-items: flex-start
            justify-content: space-between

    &__filter
        max-width: 290px
        width: 100%
        margin-right 20px
        @media(min-width 992px)
            margin-top: 10px
        &-close
            display: none

        &-selected
            padding-top: 30px

        .fbits-filtro-pai
            margin: 0
            border none
            padding: 0
            // &:first-child
            //     padding 0
        .fbits-filtro-titulo-pai
            font-size: 15px;
            color: $cor01
            line-height: 20px
            text-align: left
            text-transform: uppercase
            font-weight: 800
            display flex
            align-items: center
            justify-content: flex-start 
            padding-bottom 20px
            position relative
            border-bottom: 1px solid #cecece
            

            &:after
                 
                position: absolute
                right 0px 

        .fbits-filtro-quantidade
            color  $cor01


        .fbits-lista-filtro-filho
            display: block
            padding: 0
            .liCores
                position relative
            >li
                display flex
                align-items: center;
                font-size: 14px
                color: #333333
                font-weight: 500
                line-height 28px
                height auto
                line-height: 18px
                overflow hidden
                margin-bottom 5px
                
                input
                    appearance none
                    width: 15px
                    height 15px
                    border: 1px solid #cecece 
                    margin 0
                    margin-right 5px
                    
                    &:checked
                        background: $cor02
                    
                label 
                    margin 0
                    font-size: 14px;
                    line-height: 30px;
                    color: #333333;
                    font-weight: 500;

            .fbits-filtro-ver-mais
                text-align: left
                font-size: 12px
                color: #373535
                font-weight: bold
        .fbits-filtro-filho-item
            display: block

        .fbits-componente-filtros-selecionados
            > ul
                padding-bottom: 30px
                border-bottom: 1px solid #E7E7E7
        .fbits-componente-filtros-selecionados-titulo
            font-size: 15px
            color: #353535
            font-weight: bold
            line-height: 19px
        .fbits-componente-filtro-selecionados-pai
            padding: 5px 0
            .tituloPai
                font-size: 13px
                color: #353535
                line-height: 16px
                text-transform: none
                &:after
                    display: none
            .fbits-componente-filtro-selecionados-remover
                display: none
        .fbits-componente-filtro-selecionados-filho.checked
            display: flex
            align-items: center
            justify-content: space-between
            >a
                order: 1
                display: block
                width: 25px
                height 25px
            span 
                font-size: 13px
                color: #353535
                line-height: 16px
                text-transform: none
            .qtde
                display: none
        span.fbits-filtro-limpar
            background: #353535
            border: 1px solid #373535
            border-radius: 1px
            font-family: 'Exo', sans-serif
            font-size: 15px
            color: #FFFFFF
            letter-spacing: -0.02px
            text-align: center
            width: 100%
            font-size: 0px
            height 40px
            line-height: 40px
            margin-top: 25px
            &:before
                content: 'LIMPAR FILTROS'
                font-size: 15px
                color: #ffffff
    &__products
        width: 80%
        >span#fbits-grupo-pagina-1
            display: flex!important
            flex-wrap: wrap
            padding-top: 30px
            justify-content: space-between
        .fbits-item-lista-spot 
            width 33%
            min-height: 445px
    &__footer
        &-container
            max-width: 1190px
            width: 100%
            margin: 0 auto
        &-wrapper
            display: flex
            align-items: center
            justify-content: center
            flex-wrap: wrap
            padding-top: 30px
            margin-top: 30px
    &__text
        padding-top: 40px
        flex: 1 1 100%
        font-size: 13px
        color: #373535
        line-height: 21px
        padding-bottom: 30px
    &__btn-mobile
        display: none 

body.fbits-categoria 
    padding-top: 189px

.category__content 
    margin-top: 10px


//mobile
@media(max-width : 991px)
    .fbits-categoria
        padding-top: 100px
        .header__topbar
            display: none
    .category
        &__banner
            display: none
        &__products
            width: 100%
        &__filter
            position: fixed
            top: 0
            left 0
            transition: all .3s
            transform: translate3d(-100%, 0, 0)
            z-index: 99
            height: 100vh
            max-width: 100%
            background: rgba(0,0,0, 0.75)
            &-wrapper
                max-width: 250px
                padding: 15px
                width: 100%
                background: #fff
                height: 100vh
                overflow auto
            &-close
                display: block
                width: 45px
                height 45px
                position absolute
                top: 0px
                left 255px
                color $cor02
                font-size 25px
                line-height normal
                display: flex;
                align-items: center;
                justify-content: center;
                font-weight: bold;
        &__header
            &-wrapper
                padding-top: 25px
                > *
                    flex: 1 1 100%
                    text-align: left
                    margin: 5px 0
                .category__pagination
                    display: none
                .fbits-ordenar
                    justify-content: center
        &__btn-mobile
            display: flex
            align-items: center
            justify-content: center
            max-width 290px
            width: 100%
            height 40px
            margin: 0 auto
            background: $cor01
            font-size: 14px
            color: #FFFFFF
            text-transform: uppercase
            font-weight: bold
            transition: all .3s
            margin: 15px auto
            cursor: pointer
            position: fixed
            bottom: 0
            z-index: 99
            width: 100%
            max-width: 100%
            left: 0
            margin: 0
            &:hover
                background: #ffffff
                color: #333333
                border: 1px solid #333333
        &__footer
            padding-bottom: 55px
            &-wrapper
                justify-content: center
                border: none
                width: 100%
            .category__inforesult, .category__text
                display: none
        &__content 
            margin-top: 0px;
            padding: 0 15px;
    .filter__open
        .category__filter
            transform: translate3d(0, 0, 0)
@media(max-width : 525px)
    .category__products
        .fbits-item-lista-spot
            flex: 1 1 49%

@media(max-width : 320px)
    .category__products
        .fbits-item-lista-spot
            flex: 1 1 100%

.header__cat-order 
    display: flex
    align-items: center
    justify-content: space-between
    border-bottom: 1px solid #cecece
    padding-bottom: 20px

.categoria__name
    display block
    font-size: 20px
    color: $cor01
    line-height: 27px
    text-align: left 
    text-transform: uppercase
    font-weight: 800 s
    padding-bottom 0px 
    position relative


    &-span
        display block
        font-weight: 800
    
    .fbits-item-lista-spot.fbits-item-lista-spot-empty
        display none

li.fbits-filtro-pai.category__filter-options-item
    margin-bottom 30px!important


.content-category
    padding-bottom 15px
    border-bottom: 1px solid #e1e1e1

    &__container
        max-width 1245px
        margin 0 auto
    &__wrapper
        .breadc
            ol
                li
                    display none
                    &:last-child
                        display block
                        text-align center
                        a
                            font-weight 800 !important
                            font-size: 22px
                            color: #0080c7
        .content    
            .fbits-conteudo-item
                max-width 1000px
                font-size: 12px
                color: #7f7f7f
                text-align center
                margin 0 auto
                @media(max-width 1300px)
                    max-width 800px
                    font-size: 12px


.container-categoria
    @media(max-width 767px)
        padding-top 0 !important
.fbits-topo-categoria
    position relative
    &:before
        content: 'Refinar busca'
        font-size: 18px
        font-weight bold
        color: #0080c7
        position absolute
        top: 30px
        left: 60px
        @media(max-width 1300px)
            left: 20px
    li.filtroPai, li.fbits-filtroSelecionados-pai, li.fbits-filtro-pai
        border-bottom: none
    @media(max-width 991px)
        .filtroTitle.title.abert
            top: 25px !important
            span.ico-filtrar-busca.ico-filtrar-busca-fecha
                position: absolute !important;
                left: -50px !important;
                top: 0;
                visibility: visible;
                opacity: 1;
                background-color: #fff;
                z-index: 999999999;
                height: 46px;
                border-radius: 5px;
                border: 1px solid #0080c7;
                width: 46px;
                background-image url('data:image/svg+xml;base64,')
                background-position: center 
                background-repeat: no-repeat
                background-size: 20px
